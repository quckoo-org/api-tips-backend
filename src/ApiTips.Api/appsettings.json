{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "App": {
    "Host": "0.0.0.0",
    "Ports": {
      "Http1": 30001,
      "Http2": 30002
    },
    "Commit": "",
    "Domain": "beta.api-tips.quckoo.net"
  },
  "Postgres": {
    "Host": "<helm-value>",
    "Port": "5432",
    "User": "postgres",
    "Database": "<helm-value>"
  },
  "Redis": {
    "Host": "<helm-value>",
    "Port": "6379",
    "AbortConnect": false,
    "ConnectRetry": 5,
    "ConnectTimeoutSeconds": 3
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "",
    "Audience": "",
    "JwtExpirationInHours": 1,
    "RefreshExpirationInHours": 8760
  },
  "Smtp": {
    "Host": "mail.hosting.reg.ru",
    "Port": 587,
    "From": "admin@quckoo.net",
    "UseSsl": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "EventSource": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithCaller"
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "",
          "apiKey": ""
        }
      }
    ]
  }
}
