{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "App": {
    "Host": "0.0.0.0",
    "Ports": {
      "Http1": 30001,
      "Http2": 30002
    },
    "Commit": "",
    "DomainBackEnd": "beta.api-tips.api.quckoo.net",
    "DomainFrontEnd": "beta.api-tips.quckoo.net"
  },
  "Postgres": {
    "Host": "<helm-value>",
    "Port": "5432",
    "User": "postgres",
    "Database": "<helm-value>"
  },
  "Redis": {
    "Host": "<helm-value>",
    "Port": "6379",
    "AbortConnect": false,
    "ConnectRetry": 5,
    "ConnectTimeoutSeconds": 3
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "",
    "Audience": "",
    "JwtExpirationInHours": 1,
    "RefreshExpirationInHours": 8760
  },
  "BankRequisites": {
    "BankName": "FakeBank",
    "BankAddress": "1234 Imaginary St, Nowhere City",
    "Swift": "FAKEBANKXX",
    "AccountNumber": "9876543210",
    "Iban": "FAKEIBAN1234567890",
    "AdditionalInfo": "Some additional bank details",
    "CurrencyType": "USD",
    "IsBanned": "False"
  },
  "CryptoRequisites": {
    "Address": "0xFAKE1234ABCD5678EFGH91011IJKL",
    "Wallet": "WALLET1234FAKE",
    "Token": "super_secret_fake_token",
    "Crypto": "USDT",
    "IsBanned": "False"
  },
  "Smtp": {
    "Host": "mail.hosting.reg.ru",
    "Port": 587,
    "From": "admin@quckoo.net",
    "UseSsl": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "EventSource": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithCaller"
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "",
          "apiKey": ""
        }
      }
    ]
  }
}
